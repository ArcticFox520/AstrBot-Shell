function ManageSession(){
case "$1" in
  start)
    tmux new -s "$2" -d "$3" > /dev/null
    return $?
  ;;
  stop)
    tmux kill-session -t "$2" > /dev/null
    return $?
  ;;
  restart)
    tmux kill-session -t "$2" && tmux new -s "$2" -d "$3" > /dev/null
    return $?
  ;;
  log)
    tmux attach -t "$2" > /dev/null
    return $?
  ;;
  list)
    tmux ls | grep -q "$2"
    return $?
  ;;
esac
}